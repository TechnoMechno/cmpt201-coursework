#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
  char *line = NULL;
  size_t len = 0;

  printf("Please enter some text: ");
  ssize_t size = getline(&line, &len, stdin);
  char *token;
  char *saveptr;

  if (line != NULL) {
    token = strtok_r(line, " ", &saveptr);
  }
  printf("%s\n", token);
  while (token != NULL) {
    printf("%s\n", token);
    token = strtok_r(NULL, " ", &saveptr);
  }

  free(line);
  return 0;
}
